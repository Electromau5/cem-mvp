  <div class = "container-fluid">
    <div class = "row">
		<% @review.each do |review| %>
		  <div class = "col-xs-12 col-md-3 col-sm-6">
		    <div class = "thumbnail">
            	<%= link_to image_tag(review.image.url(:medium)), p %>
            	<p><%= review.title %></p>
		    </div>
          </div>
		<% end %>
	</div>
  </div>	  
 
		