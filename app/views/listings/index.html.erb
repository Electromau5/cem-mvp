  <div class = "container-fluid">
    <div class = "row">
		<% @listing.each do |listing| %>
		  <div class = "col-xs-12 col-md-3 col-sm-6">
		    <div class = "thumbnail">
            <%= link_to image_tag(listing.image.url(:medium)), listing %>
            <p><%= listing.name %></p>
		    </div>
          </div>
		<% end %>
	</div>
  </div>	  
